{
	"name": "project",
	"version": "0.0.1",
	"private": true,
	"license": "muonw-0",
	"scripts": {
		"dev": "vite dev",
		"buildLocal": "vite build --mode=development && rm -rf ../_docs && mv docs _docs && mv _docs ../",
		"buildProduction": "vite build --mode=production && rm -rf ../docs && mv docs ../",
		"package": "svelte-kit sync && svelte-package",
		"prepublishOnly": "if [ -z $MU_NPM_PERMIT ]; then echo 'Please use `publishLocal` or `publishPublic`' && exit 1; fi",
		"publishLocal": "../_/NPM_REGISTRY_LOCAL",
		"resetLocal": "../_/NPM_REGISTRY_LOCAL_RESET",
		"publishPublic": "../_/NPM_REGISTRY_PUBLIC",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"updateDeps": "npx npm-check-updates -u"
	},
	"exports": {
		".": {
			"types": "./index.d.ts",
			"svelte": "./index.js"
		},
		"./*": "./*"
	},
	"devDependencies": {
		"@muonw/mascara": "0.1.11",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.15.9",
		"@sveltejs/package": "^2.0.2",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"sass": "^1.62.1",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.3"
	},
	"svelte": "./index.js",
	"types": "./index.d.ts",
	"type": "module"
}
